{
	"name": "david63/registercheck",
	"type": "phpbb-extension",
	"description": "Checks if a username and email address (if email reuse is not allowed) are available and that the passwords match when registering.",
	"version": "2.1.0",
	"time": "2017-01-12",
    "keywords": [
        "phpbb",
        "extension",
        "username",
		"email",
		"register"
    ],
	"license": "GPL-2.0-only",
	"authors": [{
		"name": "David Wood",
		"role": "Developer"
	}],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Registration check",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/register_check_version_file.json"
    	}
	}
}
